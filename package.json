{
  "name": "@diplodoc/documentation-template",
  "version": "0.0.0",
  "description": "template for your diplodoc documentation with hot reload development server",
  "scripts": {
    "start": "npm run build:docs && node scripts/serve.js",
    "build:docs": "yfm --input docs --output docs-html --assets-dest docs-html --assets-include 'images/**'"
  },
  "dependencies": {
    "@diplodoc/cli": "^4.55.1",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "glob": "^10.3.4",
    "lodash": "^4.17.21",
    "open": "^8.4.2",
    "parse5": "^7.1.2",
    "parse5-utils": "^2.0.0",
    "serve-static": "^1.15.0"
  }
}
